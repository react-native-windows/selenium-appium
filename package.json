  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-native-windows/selenium-appium.git"
  },
  "scripts": {
    "prepublishOnly": "tsc -p ./ --outDir dist/",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "PageObject",
    "selenium",
    "selenium-webdriver",
    "appium",
    "jest",
    "e2e test"
  ],
  "author": "Canhua Li",
  "license": "MIT",
  "devDependencies": {
    "@types/selenium-webdriver": "^4.0.10",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "^2.22.1",
    "selenium-webdriver": "^4.0.0-alpha.8",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "selenium-webdriver": "^4.0.0-alpha.7"
  },
  "bugs": {
    "url": "https://github.com/react-native-windows/selenium-appium/issues"
  },
  "homepage": "https://github.com/react-native-windows/selenium-appium#readme",
